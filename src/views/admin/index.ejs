<%-include('../partials/head',{
    title:"Admin: " + user[0].nombre,
    css:[]
})%>
<body>
    <header>
        <%- include('../partials/navbar')%>
    </header>
    <main>
        <%=users%>

    <table class="table table-hover">
    <thead>
        <tr>
            <th scope="col">#</th>
            <th scope="col">nombre</th>
            <th scope="col">apellido</th>
            <th scope="col">tipo usuario</th>
            <th scope="col"></th>
            <th scope="col"></th>

        </tr>
    </thead>
    <tbody>
        <% users.forEach((item,index) => {%>
                <tr>
                    <th scope="row"><%=index + 1%></th>
                    <td><%=item.nombre%></td>
                    <td><%=item.apellido%></td>

                    <td><a href="/admin/edit/<%= item.id %>" class="btn btn-primary">mas...</a></td>

                </tr>
        <%}) %>
        </tbody>
    </table>
</main>



    <%-include('../partials/footer',{ js:[] })%>