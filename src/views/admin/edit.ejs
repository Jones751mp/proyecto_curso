<%-include('../partials/head',{
    title: user[0].nombre,
    css:[]
})%>

<body>

<header>
    <%-include('../partials/navbar') %>
</header>


<main>
    <form action="/admin/edit/<%=user[0].id%>" method="post">
        <div class="mb-3">
            <input type="text" name="nombre" value="<%=user[0].nombre%>"/>
        </div>
        <div class="mb-3">
            <input type="text" name="apellido" value="<%=user[0].apellido%>"/>
        </div>
        <div class="mb-3">
            <input type="email" name="email" value="<%=user[0].email%>"/>
        </div>
        <div class="mb-3">
            <input type="text" name="telefono" value="<%=user[0].telefono%>"/>
        </div>
        <input type="submit" value="Actualizar">
    </form>
</main>


<%-include('../partials/footer',{
    js:[]
})%>